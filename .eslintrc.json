{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "space-before-function-paren": [
      "warn",
      "never"
    ],
    "no-async-promise-executor": "off"
  },
  "plugins": [
    "html"
  ]
}
